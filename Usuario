/**
 *
 * @author marflord
 */
public class Usuario {
    public abstract class Usuarios{
        String nombreCompleto;
        String password;
        String estadoUsuario;
        String direccion;
        String ciudad;
        String email;
        String telefono;
        
        public Usuarios (String nC, String p, String eU,String d, String c, String e, String t){
            this.nombreCompleto=nC;
            this.password=p;
            this.estadoUsuario=eU;
            this.direccion=d;
            this.ciudad=c;
            this.email=e;
            this.telefono=t;
            
           }
        
       
        
    } 
   public abstract Operario extends Usuario {
                
        
        
     
        
                
        
    }
    
}
